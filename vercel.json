{
	"framework": null,
	"buildCommand": "rm -rf node_modules/.cache && npm run build",
	"installCommand": "npm ci --omit=optional && npm install @rollup/rollup-linux-x64-gnu @sveltejs/adapter-vercel --no-save",
	"build": {
		"env": {
			"NODE_OPTIONS": "--max-old-space-size=8192",
			"NODE_ENV": "production",
			"VITE_BUILD_TARGET": "vercel",
			"PUBLIC_API_BASE_URL": "https://swift-ai-assist.e8demo.com",
			"PUBLIC_CUSTOM_API_BASE_URL": "https://swift-ai-assist.e8demo.com",
			"PUBLIC_FRONTEND_ONLY": "true",
			"OLLAMA_BASE_URL": "https://swift-ai-assist.e8demo.com/ollama",
			"SCARF_NO_ANALYTICS": "true",
			"DO_NOT_TRACK": "true",
			"ANONYMIZED_TELEMETRY": "false"
		}
	}
}